{
    "compilerOptions": {
        "baseUrl": "./",
        "jsx": "react",
        "paths": {
            "Style/*": [
                "src/style/*",
                "src\\style/*"
            ],
            "@Component/*": [
                "src/components/*",
                "src\\components/*"
            ],
            "Component/*": [
                "src/components/*",
                "src\\components/*",
                "src\\component/*",
                "src\\components\\*"
            ],
            "Route/*": [
                "src/route/*",
                "src\\route/*"
            ],
            "Store/*": [
                "src/store/*",
                "src\\store/*"
            ],
            "Util/*": [
                "src/util/*",
                "src\\util/*"
            ],
            "Query/*": [
                "src/query/*",
                "src\\query/*"
            ],
            "Type/*": [
                "src/type/*",
                "src\\type/*"
            ],
            "/*": [
                "src/*"
            ],
            "Pages/*": [
                "src\\pages/*"
            ]
        }
    }
}